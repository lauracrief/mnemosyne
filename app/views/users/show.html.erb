<div class="container">
  <div class="row">
    <div class="row gutters">
      <div class="profil">
        <div class="col col-4">
          <%= image_tag "timothee.jpg", class:"avatar" %>
        </div>

        <div class="user-data">
          <%= @user.first_name %> <br>
          <%= @user.last_name %> <br>
          <%= @user.email %> <br>
          <%= @user.website %> <br>
          <%= @user.bio %>
        </div>
      </div>
    </div>

    <div class="row">
      <nav class="tabs" data-component="tabs">
        <ul>
          <div class="col col-3">
            <li><a href="#my_thesis">Mon Mémoire</a></li>
          </div>
          <div class="col col-3">
            <li><a href="#my_bookmarks">Mes favoris</a></li>
          </div>
          <div class="col col-3">
            <li><a href="#settings">Paramètres et Confidentialité</a></li>
          </div>
        </ul>
          <div id="my_thesis">Mémoire display test</div>
          <div id="my_bookmarks">Mes Favoris display test</div>
          <div id="settings">Paramètres display test</div>
      </nav>
    </div>
  </div>
</div>
